<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0+" href="jgridstart.jnlp" codebase="@wwwbase@">
 <information>
  <title>jGridstart</title>
  <vendor>Nikhef</vendor>
  <homepage href="http://jgridstart.nikhef.nl/"/>
  <description>Setup your computer to work with the grid</description>
 </information>
 <security>
  <all-permissions/>
 </security>
 <resources>
  <j2se href="http://java.sun.com/products/autodl/j2se" version="1.5+"/>
  <jar href="jgridstart.jar"/>
  <jar href="bcprov.jar"/>
  <!-- program defaults, customize these for your site (properties are prefixed
       with "jnlp." to avoid the need for a signed jnlp) -->
  <property name="jnlp.jgridstart.defaults.country" value="NL"/>
  <property name="jnlp.jgridstart.defaults.level" value="medium"/>
  <!-- end of program defaults -->
 </resources>
 <application-desc main-class="@main@"/>
</jnlp>
