<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0+" href="jgridstart.jnlp" codebase="@wwwbase@">
 <information>
  <title>jGridstart</title>
  <vendor>Nikhef</vendor>
  <homepage href="http://jgridstart.nikhef.nl/"/>
  <description>Setup your computer to work with the grid</description>
 </information>
 <security>
  <all-permissions/>
 </security>
 <resources>
  <j2se href="http://java.sun.com/products/autodl/j2se" version="1.5+"/>
  <jar href="jgridstart.jar"/>
  <!-- BouncyCastle requires full permissions, which brings up an additional
       security dialog for the user; this can be remedied for the Sun JRE, see
       http://jgridstart.nikhef.nl/Bouncycastle_and_Java_Web_start -->
  <extension name="BouncyCastle cryptography library" href="bcprov.jnlp"/>
  <property name="java.security.policy" value="@wwwbase@security.policy"/>
  <!-- program defaults, customize these for your site -->
  <property name="jgridstart.defaults.country" value="NL"/>
  <property name="jgridstart.defaults.level" value="medium"/>
  <!-- end of program defaults -->
 </resources>
 <application-desc main-class="@main@"/>
</jnlp>
